service livedisplay-hal-2-0-m1926 /system/bin/hw/aosp.livedisplay@2.0-service.m1926
    class late_start
    user system
    group system

on post-fs-data
    mkdir /data/vendor/display
    chown system system /data/vendor/display

on property:sys.boot_completed=1
    exec - system system -- /system/bin/aosp.livedisplay@2.0-init.sh
